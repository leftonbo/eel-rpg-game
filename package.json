{
  "name": "eel-feed",
  "version": "1.0.0",
  "description": "A turn-based RPG game where you play as Elnal the eel fighting against giant bosses",
  "main": "dist/main.js",
  "type": "module",
  "sideEffects": [
    "**/*.css",
    "./src/main.ts"
  ],
  "scripts": {
    "build": "vite build",
    "build:analyze": "vite build --mode analyze",
    "dev": "vite",
    "start": "npm run dev",
    "preview": "vite preview",
    "clean": "rimraf dist",
    "lint": "eslint src",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "boss-overview": "tsx scripts/boss-overview.ts"
  },
  "keywords": [
    "game",
    "rpg",
    "turn-based",
    "typescript",
    "browser"
  ],
  "author": "LefTonbo",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=22.0.0"
  },
  "devDependencies": {
    "@types/node": "^24.1.0",
    "@typescript-eslint/eslint-plugin": "^8.36.0",
    "@typescript-eslint/parser": "^8.36.0",
    "ejs": "^3.1.10",
    "eslint": "^9.31.0",
    "gray-matter": "^4.0.3",
    "rimraf": "^6.0.0",
    "rollup-plugin-visualizer": "^6.0.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "vite": "^7.0.0",
    "vite-plugin-ejs": "^1.7.0",
    "vite-plugin-live-reload": "^3.0.5",
    "vite-plugin-markdown": "^2.2.0",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "marked": "^16.1.1",
    "node": "^20.19.4"
  }
}
