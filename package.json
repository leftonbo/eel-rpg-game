{
  "name": "eel-feed",
  "version": "1.0.0",
  "description": "A turn-based RPG game where you play as Elnal the eel fighting against giant bosses",
  "main": "dist/main.js",
  "type": "module",
  "sideEffects": [
    "**/*.css",
    "./src/main.ts"
  ],
  "scripts": {
    "build": "npm run build:templates && webpack --mode production",
    "build:analyze": "npm run build:templates && webpack --mode production --env analyze",
    "build:templates": "node scripts/build-templates.js",
    "dev": "npm run build:templates && webpack serve --mode development --open",
    "start": "npm run dev",
    "clean": "rimraf dist",
    "lint": "eslint src",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "game",
    "rpg",
    "turn-based",
    "typescript",
    "browser"
  ],
  "author": "LefTonbo",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^8.36.0",
    "@typescript-eslint/parser": "^8.36.0",
    "copy-webpack-plugin": "^13.0.0",
    "css-loader": "^6.8.0",
    "ejs": "^3.1.10",
    "eslint": "^9.31.0",
    "html-webpack-plugin": "^5.5.0",
    "rimraf": "^5.0.0",
    "style-loader": "^3.3.0",
    "ts-loader": "^9.4.0",
    "typescript": "^5.0.0",
    "webpack": "^5.88.0",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^5.1.0",
    "webpack-dev-server": "^5.2.2"
  }
}
